    "import string\n",
    "\n",
    "def column_to_number(column_name):\n",
    "    letters = string.ascii_uppercase\n",
    "    result = 0\n",
    "    for char in column_name:\n",
    "        result = result * 26 + letters.index(char) + 1\n",
    "    return result\n",
    "\n",
    "# Example usage\n",
    "column_number = column_to_number(\"AB\")\n",
    "print(column_number-1)\n"
   ]
